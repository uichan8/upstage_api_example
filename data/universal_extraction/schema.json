{
  "type": "json_schema",
  "json_schema": {
    "name": "document_schema",
    "schema": {
      "type": "object",
      "properties": {
        "공급자 등록번호": {
          "type": "string",
          "description": "공급자의 사업자 등록 번호"
        },
        "거래 일자": {
          "type": "string",
          "description": "영수증 기준으로 거래 된 일자"
        },
        "합계 금액": {
          "type": "string",
          "description": "총 내야 할 금액"
        },
        "품목 세부사항": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "품목": {
                "type": "string",
                "description": "구입한 물건의 이름"
              },
              "규격": {
                "type": "string",
                "description": "품목이 고유적으로 가지고있는 단위 (없을 수 있음)"
              },
              "단위": {
                "type": "string",
                "description": "수량의 단위"
              },
              "수량": {
                "type": "number",
                "description": "구입 갯수"
              },
              "단가": {
                "type": "number",
                "description": "품목 하나의 가격 (금액 + 세액)"
              },
              "금액": {
                "type": "number",
                "description": "해당 품목의 값"
              },
              "세액": {
                "type": "number",
                "description": "해당 품목의 부가가치세"
              },
              "합계": {
                "type": "number",
                "description": "금액에 새액을 더한 값"
              }
            }
          }
        }
      }
    }
  }
}